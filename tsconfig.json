{
    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["vitest/globals", "node"],
      "jsx": "react-jsx",
      "noEmit": true,
      "resolveJsonModule": true
    },
    "include": ["src", "tests", "frontend_web/src"],
    "exclude": ["node_modules", "**/node_modules/*", "**/*.js"]
  }
  