{
    "extends": "./tsconfig.path.json",
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Node",
      "allowImportingTsExtensions": true,
      "noEmit": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["vitest/globals", "node"],
      "jsx": "react-jsx"
    },
    "include": ["src", "tests", "frontend_web/src"]
  }
  